{
  "name": "securephotoshare",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:local-debug": "SET ENVFILE=.env.local && react-native run-android --mode=devDebug --appIdSuffix=dev",
    "android:dev-debug": "SET ENVFILE=.env.dev && react-native run-android --mode=devDebug --appIdSuffix=dev",
    "android:test-debug": "SET ENVFILE=.env.test && react-native run-android --mode=betaDebug --appIdSuffix=beta",
